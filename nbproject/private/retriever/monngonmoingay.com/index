						$(document).ready(function() {
							var Today = $.datepicker.formatDate('dd/mm/yy/', new Date());
							$.get('get-lich-phat-song/?ngay='+Today,function(data){
								var check = data;
								if(check != 'not_found')
								{									
									$('#video-youtube').html('<iframe id="video-iframe" src="https://www.youtube.com/embed/'+data+'?wmode=transparent" frameborder="0" allowfullscreen></iframe>');					
